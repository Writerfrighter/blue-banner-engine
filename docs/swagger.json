{
    "swagger": "2.0",
    "info": {
        "description": "This is the API server for the Blue Banner Engine, providing FRC match predictions.",
        "title": "Blue Banner Engine (BBE) API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/predict/{match_key}": {
            "get": {
                "description": "Retrieves a full prediction for a given FRC match key from the BBE prediction service.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "predictions"
                ],
                "summary": "Get Match Prediction",
                "parameters": [
                    {
                        "type": "string",
                        "description": "FRC Match Key (e.g., 2024txda_qm1)",
                        "name": "match_key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful prediction",
                        "schema": {
                            "$ref": "#/definitions/main.PredictionResponseJSON"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error (e.g., gRPC call failed)",
                        "schema": {
                            "$ref": "#/definitions/main.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string"
                }
            }
        },
        "main.PredictionResponseJSON": {
            "type": "object",
            "properties": {
                "match_key": {
                    "type": "string",
                    "example": "2024txda_qm1"
                },
                "predicted_scores": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "integer"
                    },
                    "example": {
                        "blue": 112,
                        "red": 95
                    }
                },
                "predicted_winner": {
                    "type": "string",
                    "example": "blue"
                },
                "win_probability": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "number"
                    },
                    "example": {
                        "blue": 0.79,
                        "red": 0.21
                    }
                }
            }
        }
    }
}